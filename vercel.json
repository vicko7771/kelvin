{
  "version": 2,
  "builds": [
    {
      "src": "production/payments.php",
      "use": "@vercel/php"
    },
    {
      "src": "production/test_xtracky.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/production/payments.php",
      "dest": "production/payments.php"
    },
    {
      "src": "/production/(.*)",
      "dest": "production/payments.php"
    }
  ]
}
